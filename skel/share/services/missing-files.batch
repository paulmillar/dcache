#
# Missing Files service
#
# An optional, pluggable component that allows dCache to respond to
# missing files.
#

onerror shutdown

check -strong missing-files.thread-count

check missing-files.plugin.list

create org.dcache.cells.UniversalSpringCell ${cell.name} \
     "classpath:org/dcache/missingfiles/missingfiles.xml -export"
